			                			var Common_UINode_No_Aside = function(options) {
			            	             	
			            	            	var uiNode = GIAPI.Common_UINode(options);	
			            	            	
			            	            	options.asideDom = function(node,options,asideId){                	
			            	            	};
			            	            	
			            	            	options.onAsideReady = function(aside,node){               	
			            	            	};
			            	
			            	            	return uiNode;
