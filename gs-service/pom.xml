<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>eu.essi_lab</groupId>
		<artifactId>DAB</artifactId>
		<version>1.0-SNAPSHOT</version>
	</parent>

	<name>ESSI-Lab GI-suite Service</name>
	<artifactId>gs-service</artifactId>
	<packaging>war</packaging>

	<properties>
		<hostname>localhost</hostname>

		<!-- To override this port value when starting jetty:run: - open the
		jetty:run 
			Maven Build in the Eclipse "Debug Configurations" menu - in the Main tab 
			add "Parameter name": jetty.port with the number of the desired port -->
		<jetty.stop_port>9091</jetty.stop_port>
		<contextPath>gs-service</contextPath>

		<!-- baseAppServerURL is used in the generate profile, to hack the
		client stubs so that the default constructors address the expected URLs -->
		<baseAppServerURL>http://${hostname}:${jetty.port}/${contextPath}/</baseAppServerURL>
		<maven.build.timestamp.format>yyyy-MM-dd</maven.build.timestamp.format>
		<timestamp>${maven.build.timestamp}</timestamp>
		<jetty.version>9.4.54.v20240208</jetty.version>
		<configuration.url />

	</properties>

	<build>
		<finalName>${contextPath}</finalName>

		<resources>
			<resource>
				<directory>src/main/resources/</directory>
				<filtering>false</filtering>
				<excludes>
					<exclude>docker/**</exclude>
					<exclude>deploy/**</exclude>
					<exclude>**/*.xml</exclude>
					<exclude>**/*.jsp</exclude>
					<exclude>**/*.properties</exclude>
				</excludes>
			</resource>

			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
				<includes>
					<include>**/*.xml</include>
					<include>**/*.jsp</include>
					<include>**/*.properties</include>
				</includes>
			</resource>
		</resources>

		<plugins>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-antrun-plugin</artifactId>
				<executions>
					<execution>
						<phase>package</phase>
						<goals>
							<goal>run</goal>
						</goals>
					</execution>
				</executions>
			</plugin>

			<plugin>
				<artifactId>maven-resources-plugin</artifactId>
				<!--version>2.4.3</version -->
				<executions>
					<execution>
						<id>copy-license</id>
						<phase>process-resources</phase>
						<goals>
							<goal>copy-resources</goal>
						</goals>
						<configuration>
							<outputDirectory>${basedir}/target/classes/META-INF</outputDirectory>
							<resources>
								<resource>
									<directory>META-INF</directory>
									<filtering>true</filtering>
								</resource>
							</resources>
						</configuration>
					</execution>
				</executions>
			</plugin>

			<plugin>
				<artifactId>maven-jar-plugin</artifactId>
				<configuration>
					<archive>
						<addMavenDescriptor>false</addMavenDescriptor>
					</archive>
				</configuration>
			</plugin>

			<plugin>
				<inherited>true</inherited>
				<groupId>org.codehaus.cargo</groupId>
				<artifactId>cargo-maven2-plugin</artifactId>
			</plugin>


			<!-- Jetty Plugin -->
			<plugin>
				<groupId>org.eclipse.jetty</groupId>
				<artifactId>jetty-maven-plugin</artifactId>
				<version>${jetty.version}</version>
				<configuration>

					<stopPort>${jetty.stop_port}</stopPort>
					<stopKey>stop</stopKey>
					<webAppConfig>
						<contextPath>/${contextPath}</contextPath>

						<webInfIncludeJarPattern>
							.*/spring-[^/]*\.jar$|.*vaadin-[^/]*\.jar$|.*flow-[^/]*\.jar$|.*/togglebutton-[^/]*\.jar$|.*/multiselect-combo-box-flow-[^/]*\.jar$</webInfIncludeJarPattern>
						<containerIncludeJarPattern>
							.*/spring-[^/]*\.jar$|.*vaadin-[^/]*\.jar$|.*flow-[^/]*\.jar$|.*/togglebutton-[^/]*\.jar$|.*/multiselect-combo-box-flow-[^/]*\.jar$</containerIncludeJarPattern>

						<!-- Use war output directory to get the webpack files -->
						<allowDuplicateFragmentNames>true</allowDuplicateFragmentNames>

					</webAppConfig>

					<useTestClasspath>false</useTestClasspath>
					<dumpOnStart>false</dumpOnStart>

					<scanIntervalSeconds>0</scanIntervalSeconds>
					<jettyXml>
						${project.basedir}/src/main/webapp/WEB-INF/jetty.xml,
						${project.basedir}/src/main/webapp/WEB-INF/jetty-http.xml</jettyXml>

				</configuration>
				<dependencies>

				</dependencies>
			</plugin>


			<!-- War plugin -->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<configuration>
					<attachClasses>true</attachClasses>
					<webResources>
						<!-- packagingExcludes>truststore.jks</packagingExcludes -->
						<resource>
							<directory>target/classes</directory>
							<includes>
								<include>**/*.xml</include>
								<include>**/*.properties</include>
							</includes>
							<excludes>

								<exclude>src/main/resources/truststore.jks</exclude>
								<exclude>src/main/webapp/log.jsp</exclude>

							</excludes>
						</resource>

						<resource>
							<filtering>true</filtering>
							<directory>${basedir}/src/main/webapp</directory>
							<includes>
								<include>**/*.xml</include>
								<include>**/*.jsp</include>
								<include>**/*.properties</include>
							</includes>
						</resource>

						<resource>
							<directory>target/frontend</directory>
							<targetPath>frontend</targetPath>
						</resource>
				
					</webResources>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>build-helper-maven-plugin</artifactId>
				<version>1.4</version>
				<executions>
					<execution>
						<id>add-source</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>add-source</goal>
						</goals>
						<configuration>
							<sources>
								<source>${basedir}/target/filtered-sources</source>
							</sources>
						</configuration>
					</execution>
				</executions>
			</plugin>

			<!-- Take care of synchronizing java dependencies and imports in
			package.json and main.js files. It also creates webpack.config.js if not exists yet. -->
			<plugin>
				<groupId>com.vaadin</groupId>
				<artifactId>vaadin-maven-plugin</artifactId>
				<version>${vaadin.version}</version>
				<executions>
					<execution>
						<goals>
							<goal>prepare-frontend</goal>
						</goals>
					</execution>
				</executions>
			</plugin>

		</plugins>

		<pluginManagement>
			<plugins>
				<!--This plugin's configuration is used to store Eclipse m2e settings 
					only. It has no influence on the Maven build itself. -->
				<plugin>
					<groupId>org.eclipse.m2e</groupId>
					<artifactId>lifecycle-mapping</artifactId>
					<version>1.0.0</version>
					<configuration>
						<lifecycleMappingMetadata>
							<pluginExecutions>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>
											com.samaxes.maven
										</groupId>
										<artifactId>
											maven-minify-plugin
										</artifactId>
										<versionRange>
											[1.2,)
										</versionRange>
										<goals>
											<goal>minify</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore />
									</action>
								</pluginExecution>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>
											org.codehaus.mojo
										</groupId>
										<artifactId>
											build-helper-maven-plugin
										</artifactId>
										<versionRange>
											[1.4,)
										</versionRange>
										<goals>
											<goal>add-source</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore />
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>


