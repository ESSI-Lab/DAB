		<profile>
			<id>createdockerimage</id>
			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>
			<build>
				<plugins>
					<plugin>
						<!-- This plugin enables creation of a docker image of
						gs-service calling: -->
						<!-- mvn -Pcreatedockerimage clean package -->
						<!-- Optionally, add a configuration file from a
						profile, e.g. -Pwcs -->
						<groupId>com.spotify</groupId>
						<artifactId>dockerfile-maven-plugin</artifactId>
						<version>1.4.10</version>
						<executions>
							<execution>
								<id>default</id>
								<goals>
									<goal>build</goal>
									<goal>push</goal>
								</goals>
							</execution>
						</executions>
						<configuration>
							<dockerfile>
								${project.basedir}/src/main/resources/docker/run-configured.Dockerfile</dockerfile>
							<tag>1</tag>
							<repository>localhost/gi-suite</repository>
							<buildArgs>
								<WAR_FILE>target/${project.build.finalName}.war</WAR_FILE>
							</buildArgs>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>

		<profile>
