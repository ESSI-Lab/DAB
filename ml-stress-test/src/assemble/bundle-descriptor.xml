<?xml version="1.0" encoding="UTF-8"?>
<assembly>
    <id>bin</id>
    <formats>
         <format>zip</format>
    </formats>
    <fileSets>
        <fileSet>
            <directory>target</directory>
            <includes>
                <include>README</include>
                <include>*.bat</include>
                <include>*.sh</include>
                <include>*.vbs</include>
            </includes>
            <outputDirectory/>
            <fileMode>755</fileMode>
        </fileSet>      
    </fileSets>
    <dependencySets>
        <dependencySet>
            <outputDirectory>lib</outputDirectory>
            <includes>
                <include>*:*</include>
            </includes>
            <unpack>false</unpack>
            <scope>runtime</scope>
        </dependencySet>
    </dependencySets>
</assembly>